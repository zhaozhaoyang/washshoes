/* pages/setting/setting.wxss */
.container{
  width: 100%;
  height: 100%;
}
.cell{
  width: 100%;
}
.bottom{
  width: 100%;
  height: 100rpx;
  background: #8EC31F;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  padding: 0 40rpx;
  box-sizing: border-box;
}

.bottom>image{
  width: 60rpx;
  height: 60rpx;
}

.phone{
  height: 80rpx;
  margin-left: 40rpx;
  border-left: 2px solid #fff;
  padding-left: 40rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.question{
  font-size: 26rpx;
  color: #333;
}

.tel{
  font-size: 24rpx;
  color: #666;
  margin-top: 4rpx;
}