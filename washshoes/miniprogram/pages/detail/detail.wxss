/* pages/detail/detail.wxss */
@import '../../wxParse/wxParse.wxss';

page, .container {
  background: rgb(248, 248, 248);
  height: 100%;
}

.pick {
  width: 100%;
  height: 1000rpx;
  /* height: 80%; */
}

.active {
  border: 1px solid #8EC31F;
}

/* .cm{
  box-sizing: border-box;
  padding: 0 30rpx;
  background: #fff;
} */

swiper, .swip {
  width: 100%;
  height: 750rpx;
}

.tit {
  height: 162rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  background: rgb(255, 255, 255);
}

.h1 {
  font-size: 32rpx;
  font-weight: 500;
  margin: 15rpx 0;
}

.h2 {
  font-size: 26rpx;
}

.t1 {
  color: rgb(142, 195, 31);
  height: 30rpx;
  font-size: 30rpx;
  font-weight: 800;
}

.t2 {
  height: 23rpx;
  font-size: 24rpx;
  color: rgb(159, 159, 160);
}

.txt {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 50rpx;
}

.select {
  width: 100%;
  margin: 16rpx auto;
  background: rgb(255, 255, 255);
}

.com {
  width: 100%;
  background: rgb(255, 255, 255);
}

.rt {
  display: flex;
  align-items: center;
}

.more {
  color: #9f9fa0ff;
  font-size: 24rpx;
}

.select1 {
  height: 88rpx;
  background: #fff;
  width: 100%;
}

.cm {
  /* height: 442rpx; */
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.ch1 {
  padding-top: 20rpx;
}

.ch1 image {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  vertical-align: middle;
  border-radius: 50%;
}

.ch1 text {
  font-size: 28rpx;
  font-weight: 400;
  vertical-align: middle;
  margin-left: 24rpx;
}

.ch2 {
  font-size: 26rpx;
  font-weight: 400;
  color: rgb(159, 159, 160);
  margin-left: 80rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.ch3 {
  /* height: 60rpx; */
  font-size: 26rpx;
  font-weight: 400;
  padding-left: 80rpx;
  box-sizing: border-box;
}

.ch4 {
  display: flex;
  /* justify-content: space-between; */
  height: 234rpx;
  box-sizing: border-box;
  padding-left: 80rpx;
  align-items: center;
}

.ch4 image {
  display: block;
  margin-right: 20rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
}

.baby {
  position: relative;
  font-size: 32rpx;
  font-weight: 500;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: rgba(248, 248, 248, 1);
}

.bgc {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  height: 8rpx;
  width: 170rpx;
}

.pic {
  /* height: 839rpx; */
  width: 95%;
  margin: 0 auto 100rpx;
}

.pic image {
  display: block;
  width: 100%;
  height: 100%;
}

.common {
  width: 90%;
  height: 220rpx;
  display: flex;
  justify-content: space-between;
  margin: 20rpx auto;
}

.cent {
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;
  padding-left: 40rpx;
}

.price {
  font-size: 30rpx;
  font-weight: 500;
  color: #8ec31fff;
}

.atr {
  font-size: 22rpx;
  font-weight: 400;
}

.common image {
  display: inline-block;
}

.lt {
  width: 218rpx;
  height: 209rpx;
}

.rt {
  width: 36rpx;
  height: 36rpx;
}

.btn {
  height: 98rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  font-weight: 500;
}

.btn button {
  flex: 1;
  border-radius: 0 !important;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.btn .add {
  background: #000;
  color: #fff;
}

.sec1 {
  width: 90%;
  margin: 0 auto;
}

.notice {
  color: rgb(142, 195, 31);
  font-size: 30rpx;
  font-weight: 400;
  height: 150rpx;
  position: fixed;
  bottom: 98rpx;
  line-height: 150rpx;
  width: 100%;
  text-align: center;
}

.type {
  font-size: 30rpx;
  height: 34rpx;
  line-height: 34rpx;
  margin: 16rpx;
}

.span {
  height: 200rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 28rpx;
  width: 100%;
}

van-tabbar, .van-tabbar {
  width: 100%;
  height: 100%;
}

.van-tabbar--fixed {
  position: static !important;
}

.span1 {
  height: 200rpx;
}

.span1 van-tabbar, .span1 .van-tabbar {
  width: 100%;
  height: 100%;
}

.span van-tabbar, .span .van-tabbar {
  width: 100%;
  height: 100%;
  justify-content: flex-start;
}

.span1 van-tabbar-item, .span van-tabbar-item {
  /* width: 200rpx; */
  height: 50rpx;
  justify-content: flex-start;
  /* flex-basis: 150rpx; */
}

.van-tabbar {
  flex-wrap: wrap;
  justify-content: flex-start;
  flex-basis: 150rpx;
}

.span3 image {
  display: block;
  width: 170rpx;
  height: 170rpx;
}

.sec3 {
  width: 100%;
  height: 600rpx;
}

.van-cell__right-icon-wrap {
  color: rgb(142, 195, 31) !important;
}

.van-goods-action-icon__content {
  color: rgb(0, 0, 0) !important;
  /* background:  rgb(0,0,0)!important; */
}

.van-button--warning {
  background: rgb(0, 0, 0) !important;
  border: 0 !important;
}

.van-button--danger {
  background: rgb(142, 195, 31) !important;
  border: 0 !important;
}

.pick1 {
  height: 800rpx !important;
}

.pick1 .sec1 {
  margin: 60rpx auto;
}

.span .van-tabbar-item__text, .span1 .van-tabbar-item__text {
  padding: 16rpx 35rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 10rpx;
  min-width: 160rpx;
  box-sizing: border-box;
  text-align: center;
  color: rgba(0, 0, 0, 1);
}

.span .van-tabbar-item--active .van-tabbar-item__text,
.span1 .van-tabbar-item--active .van-tabbar-item__text {
  background: rgb(142, 195, 31);
  color: rgb(255, 255, 255);
}

button[type=primary] {
  background: rgb(142, 195, 31) !important;
}

.van-popup {
  border-radius: 10rpx 10rpx 0 0;
}

.design,.choice {
  width: 100%;
  padding: 16rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.design>view {
  padding: 0 16rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 4px;
  font-size: 12px;
  margin-bottom: 16rpx;
  background: #eee;
  color: #333;
}

.actives {
  background: #8EC31F !important;
  color: #fff !important;
}

.choice>view {
  display: flex;
  box-sizing: border-box;
  margin-bottom: 8rpx;
  width: 170rpx;
  height: 170rpx;
}

.choice>view>image {
  width:100%;
  height: 100%;
}
